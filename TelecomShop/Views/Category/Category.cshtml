
@{
    ViewBag.Title = "Category";
    Layout = "~/Views/Shared/_Layout.cshtml";

    var allCate = (List<Model.EF.CategoryPack>)ViewBag.allCate;
    var cate = (Model.EF.CategoryPack)ViewBag.cate;
    var listPackByCat = (List<Model.EF.Pack>)ViewBag.listPackByCat;


}

<section class="page-title" style="background-image: url(/Assets/client/images/background/9.jpg)">
    <div class="auto-container">
        <h2>Services</h2>
        <ul class="bread-crumb clearfix">
            <li><a href="index.html">Home</a></li>
            <li>Services</li>
        </ul>
    </div>
</section>
<!--End Page Title-->
<!-- Services Section Two -->
<section class="services-page-section">
    <div class="auto-container">

        <!-- Sec Title -->
        <div class="sec-title centered">
            <div class="separator"></div>
            <h2>All Category</h2>
        </div>
        <!-- End Sec Title -->

        <div class="four-item-carousel-two owl-carousel owl-theme">

            @foreach (var item in allCate)
            {
                <div class="service-block-two">
                    <div class="inner-box">
                        <div class="color-layer"></div>
                        <div class="icon-layer-one" style="background-image: url(/Assets/client/images/background/pattern-19.png)"></div>
                        <div class="icon-layer-two" style="background-image: url(/Assets/client/images/background/pattern-20.png)"></div>
                        <div class="icon"><img src="/Assets/client/images/icons/service-4.png" alt="" /></div>
                        <h4><a href="services-detail.html">@item.catName</a></h4>
                        <div class="text">Lorem ipsum dolor sit amet them elit seding doeiumod Donec modern</div>
                        <a class="learn-more" href="services-detail.html">Learn More</a>
                    </div>
                </div>
            }

            <!-- Service Block Two -->




        </div>

    </div>
</section>
<!-- End Services Section Two -->
