<!-- Pricing Section Two / Style Three -->
<section class="pricing-section-two style-three" style="background-image: url(/Assets/client/images/background/pattern-7.png)">
    <div class="auto-container">
        <!-- Sec Title -->
        <div class="sec-title centered">
            <div class="separator"></div>
            <h2>List Pack @cate.catName</h2>
        </div>
        <!-- End Sec Title -->

        @foreach (var item in listPackByCat)
        {
            <div class="price-block-two">
                <div class="inner-box">
                    <!-- Title Box -->
                    <div class="title-box">
                        <div class="title">@cate.catName</div>
                        <h4><a href="price.html">@item.packName</a></h4>
                        <div class="text">Choose from a range of fast, reliable Internet speeds to fit your needs</div>
                    </div>
                    <!-- End Title Box -->
                    <!-- Middle Content -->
                    <div class="middle-content">
                        <div class="middle-inner">
                            <ul class="icon-list">
                                <li><span class="icon"><img src="/Assets/client/images/icons/service-1.svg" alt="" /></span></li>
                                <li><span class="icon"><img src="/Assets/client/images/icons/service-2.svg" alt="" /></span></li>
                            </ul>
                            <ul class="price-list">
                                <li>@item.Description</li>
                                <li>Fast Connect</li>
                                <li>Unlimited devices</li>
                            </ul>
                        </div>
                    </div>
                    <!-- End Middle Content -->
                    <!-- Price Box -->
                    <div class="price-box">
                        <div class="price">$@item.Price<span>Per Month</span></div>
                        <a href="/@cate.catName/Pack/@item.packId" class="theme-btn btn-style-four"><span class="txt">Details</span></a>
                    </div>
                    <!-- End Price Box -->

                </div>
            </div>
        }

        <!-- Price Block Two -->
        


    </div>
</section>
<!-- End Pricing Section Two / Style Three -->
